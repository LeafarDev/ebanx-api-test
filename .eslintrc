{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "indent": [2, 2],
    "@typescript-eslint/ban-ts-comment": "warn",
    "comma-dangle": "error",
    "comma-style": "error",
    "semi": "error",
    "brace-style": "error",
    "camelcase": "warn",
    "no-unused-vars": "warn"
  }
}
